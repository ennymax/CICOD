<suite name="Suite">
    <test name = "UCG A" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="CICOD.ModuleUCG.ViewPendingCollections"/>
            <class name="CICOD.ModuleUCG.FilterPendingCollectionsInCorrectCredentials"/>
            <class name="CICOD.ModuleUCG.FilterPendingCollectionWithCorrectDetails"/>
            <class name="CICOD.ModuleUCG.SearchPendingCollectionWithCorrectCredentials"/>
            <class name="CICOD.ModuleUCG.SearchPendingCollectionsWithInCorrectCredentials"/>
            <class name="CICOD.ModuleUCG.DownloadPendingCollection"/>
        </classes>
    </test>

    <test name = "UCG B" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="CICOD.ModuleUCG.FilterVarienceReport"/>
            <class name="CICOD.ModuleUCG.DownloadVarienceReport"/>
        </classes>
    </test>

    <test name = "UCG C" parallel = "methods" thread-count = "2" group-by-instances="false" >
        <classes>
            <class name="CICOD.ModuleUCG.ViewSettlementReport"/>
            <class name="CICOD.ModuleUCG.FilterSettlementReportUsingDate"/>
            <class name="CICOD.ModuleUCG.SearchSettlementReportUsingIncorrectCredentials"/>
            <class name="CICOD.ModuleUCG.SearchSettlementReportUsingCorrectCredential"/>
        </classes>
    </test>
</suite>
