<suite name="Suite">
    <test name = "Queue Management" parallel = "methods" thread-count = "3" group-by-instances="false" >
    <classes>
            <class name="CICOD.ModuleWFM.QueueManagement.SUSPEND_AND_UNSUSPEND_QUEUE"/>
            <class name="CICOD.ModuleWFM.QueueManagement.CreateQueue"/>
            <class name="CICOD.ModuleWFM.QueueManagement.UpdateQUEUE"/>
        </classes>
    </test>

    <test name = "QueueTypeManagement" parallel = "methods" thread-count = "2" group-by-instances="false" >
        <classes>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.CreateQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.AddStatusToQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.SetUpEscalationForQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.AddStatusToQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.Show_HideStatusFromResource"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.Suspend_UnsuspendQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.UpdateQueueType"/>
            <class name="CICOD.ModuleWFM.QueueTypeManagement.Suspend_UnsuspendQueueTypeStatus"/>
        </classes>
    </test>

    <test name = "Manage WorkOrder Status" parallel = "methods" thread-count = "2" group-by-instances="false" >
        <classes>
            <class name="CICOD.ModuleWFM.ManageWorkOrderStatus.Suspend_UnsuspenStatus"/>
            <class name="CICOD.ModuleWFM.ManageWorkOrderStatus.UpdateStatus"/>
            <class name="CICOD.ModuleWFM.ManageWorkOrderStatus.CreateStatus"/>
        </classes>
    </test>
</suite>
