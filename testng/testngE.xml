<suite name="Suite">
    <test name = "Order History" parallel = "methods" thread-count = "3" group-by-instances="false" >
        <classes>
            <class name="com.Module.OrderHistory.Downloadorderhistory"/>
            <class name="com.Module.OrderHistory.VIEW_ORDER_HISTORY"/>
        </classes>
    </test>

        <test name = "Sales Management" parallel = "methods" thread-count = "3" group-by-instances="false" >
